{
   "manifest_version": 2,
   "minimum_chrome_version": "34",
   "name": "Agora Web Screensharing",
   "permissions": [ "desktopCapture" ],
   "short_name": "Screen sharing for Agora",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.0.1",

   "background": {
      "persistent": true,
      "scripts": [ "script.js" ]
   },
   "description": "Extension to allow screen sharing in Agora Web Application.",
   "externally_connectable": {
      "matches": ["*://gs.agora.io/*", "*://webdemo.agora.io/*", "*://webdemo.agorabeckon.com/*","*://sh.efaceboard.cn/*"]
   },
   "icons": {
      "128": "128w.png",
      "16": "16w.png",
      "48": "48w.png"
   },
   "browser_action":{
     "default_icon": "16w.png"
   },
   "key": ""
}

   